<div class="container">
	<div class="row">
		<div class="col">
			<h3>会員一覧</h3>

			<table class="table">
				<thead>
					<tr>
						<th>会員ID</th>
						<th>氏名</th>
						<th>メールアドレス</th>
						<th>ステータス</th>
					</tr>
				</thead>
				<tbody>
					<% @end_users.each do |end_user| %>
					<tr>
						<td><%= end_user.id %></td>
						<td><%= link_to admin_end_user_path(end_user) do %>
									<%= end_user.first_name + end_user.last_name %>
								<% end %>
						</td>
						<td><%= end_user.email %></td>
						<td><%= end_user.is_deleted %></td>
					</tr>
					<% end %>
				</tbody>
			</table>

		</div>
	</div>
</div>